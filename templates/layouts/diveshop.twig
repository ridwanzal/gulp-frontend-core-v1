<!doctype html>
<html lang="eng">

<head>
  {% set head_title ='Home' %}
  {% include "../components/head.twig" %}
</head>

<body>
  <div class="ex_wrapper">
    {% include "../components/navbar.twig" %}
    {% include "../components/find_diveshop.twig" %}
    <div class="row">
        <div class="col-lg-5 col-md-5">
            <div style="display:flex;margin-left:350px;margin-top:20px;margin-bottom:10px;">
                <div class="label_card">   
                    102 Result of Dive Shop
                </div>
                <div class="label_card_small" style="position: relative;
                            float: right;
                            left: 93px;">
                        <span>Sort by:</span>
                        <select class="chooser_select">
                            <option>Popularity</option>
                        </select>
                </div>
            </div>
            <div style="margin-left:350px;margin-top:10px;margin-bottom:10px; height:800px;overflow-y:scroll;">
                {% for i in 1..10 %}
                    {% include "../components/list_diveshop.twig" %}
                {% endfor %}
            </div>
        </div>
        <div class="col-lg-7 col-md-7">
            <div id="mapid" style="width:100%;height:900px;">
            </div>
        </div>
    </div>
    {% include "../components/footer.twig" %}
    {{ block('included_content') }}
  </div>
</body>
</html>

{% block content %}
{% endblock %}
<!-- scripts -->
{% block included_content %}
<script src="js/index.min.js"></script>
{% endblock %}